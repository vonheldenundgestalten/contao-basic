{
    "name": "vonheldenundgestalten/contao-basic",
    "description": "Basic stylings for all VHUG projects (for Contao 4.x)",
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Milkereit",
            "email": "mm@vonheldenundgestalten.de",
            "role": "Developer"
        },
        {
            "name": "Marko TodiÄ‡",
            "email": "drexplosiveweb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "contao/core-bundle": "4.9.*"
    },
    "replace": {
        "magmell-agentur/contao-basic": "@dev"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3"
    },
    "extra": {
        "contao-manager-plugin": "Magmell\\Contao\\Basic\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Magmell\\Contao\\Basic\\": "src"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "suggest": {
        "vonheldenundgestalten/contao-privacy": "Simple privacy handling for a small selection of typical services without external dependencies"
    }
}
